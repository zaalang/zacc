import std.stdio;
import std.math;

using std;

fn test_pow()
{
  assert(pow(2, 2) == 4);
  assert(pow(2, 3) == 8);
  assert(pow(2, 4) == 16);
  assert(pow(2, 5) == 32);
  assert(pow(2, i8(6)) == 64);
  assert(pow(2, u16(7)) == 128);
  assert(pow(2.0, 8) == 256.0);
  assert(pow(2.0, u32(9)) == 512.0);
  assert(pow(2.0, 10.0) == 1024.0);
}

fn main()
{
  #test_pow();
}
